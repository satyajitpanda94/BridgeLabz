package functionalPrograms;

import java.util.Scanner;

public class demo {
public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
	int i=sc.nextInt();
	int j=sc.nextInt();
	while(true)
	{
		try {
			System.out.println(i/j);
			sc.close();
			break;
		} catch (Exception e) {
			System.out.println("reenter");
			j=sc.nextInt();
		}
	}
}
}
